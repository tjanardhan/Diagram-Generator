}

loans: {
  shape: cylinder
  label: "Loans"
  attributes: [
    "loan_id (PK)"
    "member_id (FK)"
    "isbn (FK)"
    "loan_date"
    "due_date"
    "return_date"
  ]
}

library_member -> loans
