direction: right

library_member: {
  shape: person
  label: "Library Member"
  attributes: [
    "member_id (PK)"
    "name"
    "address"
    "phone"
    "email"
  ]
}

books: {
  shape: sql_table
  label: "Books"
  attributes: [
    "isbn (PK)"
    "title"
    "author"
    "genre"
    "publication_year"
